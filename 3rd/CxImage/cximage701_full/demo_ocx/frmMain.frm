Private Sub mnuFileOpen_Click()
    Dim sFile As String

    'If ActiveForm Is Nothing Then LoadNewDoc
    LoadNewDoc
    
    With dlgCommonDialog
        .DialogTitle = "Open"
        .CancelError = False
        'ToDo: set the flags and attributes of the common dialog control
        .Filter = "All Files (*.*)|*.*"
        .ShowOpen
        If Len(.FileName) = 0 Then
            Exit Sub
        End If
        sFile = .FileName
    End With
   
    Dim sExt As String
    Dim dot As Long
    Dim filetype As Long
    dot = InStr(1, sFile, ".", vbTextCompare)
    If dot > 0 Then sExt = LCase(Mid(sFile, dot + 1, 3))
    filetype = ActiveForm.AxImage1.StringToFormat(sExt)
    
    If sExt <> "" Then
        ActiveForm.Caption = sFile
        'ActiveForm.AxImage1.SetFrame 1
        ActiveForm.AxImage1.Load sFile, filetype
        ActiveForm.AxImage1.Stretch = False
        ActiveForm.AxImage1.Refresh
    End If

End Sub

