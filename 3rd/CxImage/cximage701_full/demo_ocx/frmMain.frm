Private Sub mnuFileSaveAs_Click()
    Dim sFile As String
    

    If ActiveForm Is Nothing Then Exit Sub
    

    With dlgCommonDialog
        .DialogTitle = "Save As"
        .CancelError = False
        'ToDo: set the flags and attributes of the common dialog control
        .Filter = "All Files (*.*)|*.*"
        .ShowSave
        If Len(.FileName) = 0 Then
            Exit Sub
        End If
        sFile = .FileName
    End With
    
    Dim sExt As String
    Dim dot As Long
    Dim filetype As Long
    dot = InStr(1, sFile, ".", vbTextCompare)
    If dot > 0 Then sExt = LCase(Mid(sFile, dot + 1, 3))
    filetype = ActiveForm.AxImage1.StringToFormat(sExt)
    If filetype <> 0 Then
        ActiveForm.Caption = sFile
        ActiveForm.AxImage1.Save sFile, filetype
    End If

End Sub

